nav.c-nav.navbar.navbar-expand-md.navbar-dark.mb-4.c-nav
	a.navbar-brand(href="#") 
		span.icon.icon-waas(role="icon")
		
	button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation" )
		//- span.icon.icon-bars(role="icon")
		span.navbar-toggler-icon

	.collapse.navbar-collapse#navbarCollapse
		ul.navbar-nav.mr-auto.c-nav__listb
			- var menu = ['Sobre nos', 'Metodologia', 'Hackateen','Blog Transmitir', 'Contato']
			each val, index in menu
				if index == 1
					li.nav-item.active.c-nav__item
						a.nav-link.c-nav__item-link(href="#")= val
				else
					li.nav-item.c-nav__item
						a.nav-link.c-nav__item-link(href="#")= val

		form
			.input-group.input-group-lg
				input.form-control(type="text" placeholder="Search" aria-label="Search")
				.input-group-append
					button.btn.btn-light.py-1(type="submit")
						span.icon.icon-search
						span.sr-only Buscar
			

