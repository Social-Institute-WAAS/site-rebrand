			.input-group.input-group-lg.c-search__wrapper
				input.form-control.c-search__input(type="text" name="search_field" placeholder="Buscar" aria-label="Buscar")
				.input-group-append.c-search__append
					button.btn.py-1(type="submit")
						span.icon.icon-search
						span.sr-only Buscar
				

