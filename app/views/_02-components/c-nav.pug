nav#navbar.c-nav.navbar.navbar-expand-lg.fixed-top.mb-4
	.container
		a.navbar-brand.c-nav__brand(href="#") 
			//- span.icon.icon-waas(role="icon")
			//- img(src="images/logo-waas.svg" style="height:70px")
			include ../_01-elements/e-logo-waas
			
		button.navbar-toggler.c-nav__btn-toggle#js-btn-toggle(type="button" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation" )
			span.icon.icon-bars(role="icon")
			//- span.navbar-toggler-icon

		.navbar-collapse.c-nav__menu.c-menu#navbarCollapse
			.c-menu__header
