					include ../_01-elements/e-logo-waas
				button.c-menu__close
					span.icon.icon-close(role="icon")

			ul.navbar-nav.mr-auto.c-nav__list
				- var menu = ['Sobre n√≥s', 'Metodologia', 'Hackateen','Blog', 'Contato']
